    <Method Name="MyMover" Id="{53ce83f8-f376-4c29-a057-798a57ce6dc6}">
      <Declaration><![CDATA[METHOD MyMover : HRESULT
VAR_INPUT
	mover	: I_XtsMover;
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF mover = 0 THEN
	MyMover := -1;
	RETURN;
END_IF
_HasMover := TRUE;
_MyMover := mover;]]></ST>
