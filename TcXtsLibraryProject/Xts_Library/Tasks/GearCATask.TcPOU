]]></Declaration>
      <Get Name="Get" Id="{870c3b62-4cf2-4c56-9b9c-d05cf4fbc241}">
        <Declaration><![CDATA[VAR
END_VAR
]]></Declaration>
        <Implementation>
          <ST><![CDATA[Done := not GearInPosCA.Busy;]]></ST>
        </Implementation>
      </Get>
    </Property>
    <Property Name="Error" Id="{b0cd652d-d067-40fd-9e01-a5029b2c46d5}">
