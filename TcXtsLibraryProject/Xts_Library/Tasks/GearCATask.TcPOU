        </Implementation>
      </Set>
    </Property>
    <Property Name="Busy" Id="{4b87a8a5-0488-49e9-9c97-79ee531d0f30}">
      <Declaration><![CDATA[PROPERTY Busy : BOOL
]]></Declaration>
      <Get Name="Get" Id="{ae7f8c84-bd44-4377-ac93-c5e80d9322fd}">
        <Declaration><![CDATA[VAR
END_VAR
