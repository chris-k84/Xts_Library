  <POU Name="ResetGroup" Id="{c0d0b360-5fec-467e-a65e-491de04b13b3}" SpecialFunc="None">
    <Declaration><![CDATA[FUNCTION_BLOCK ResetGroup  IMPLEMENTS I_Task
VAR
	groupReset			: MC_GroupReset;
