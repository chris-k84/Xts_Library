    <Method Name="Abort" Id="{5f143af2-570c-4ae8-a3e1-0058182dca35}">
      <Declaration><![CDATA[METHOD Abort : BOOL
]]></Declaration>
      <Implementation>
        <ST><![CDATA[mcRemoveAxisFromGroup.Execute := FALSE;
