    <Method Name="Cycle" Id="{8ab278dc-8dad-4664-bee0-b30bd7e80670}">
      <Declaration><![CDATA[METHOD Cycle : HRESULT
VAR_INPUT
END_VAR
]]></Declaration>
      <Implementation>
        <ST><![CDATA[IF _caGroup = 0 THEN
	Cycle := -1;
	RETURN;
END_IF

_Done := mcRemoveAxisFromGroup.Done OR _Done;
_Error := mcRemoveAxisFromGroup.Error OR _Error;
IF mcRemoveAxisFromGroup.ErrorId <> 0 THEN
	_ErrorId := mcRemoveAxisFromGroup.ErrorId;
END_IF

mcRemoveAxisFromGroup(AxesGroup := _caGroup.GroupRef);
mcRemoveAxisFromGroup.Execute := FALSE;]]></ST>
      </Implementation>
